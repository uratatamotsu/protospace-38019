<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
        <span> こんにちは、</span>
      <%= link_to current_user.name, user_path(current_user.id), class: :greeting__link%>
        <span> さん</span>
      </div>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%= render partial: "prototypes/prototype", collection: @prototypes %>
    </div>
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
    <% else%>
    <div class="card__wrapper">
      <%= render partial: "prototypes/prototype", collection: @prototypes %>
    </div>
    <% end %>
  </div>
</main>
